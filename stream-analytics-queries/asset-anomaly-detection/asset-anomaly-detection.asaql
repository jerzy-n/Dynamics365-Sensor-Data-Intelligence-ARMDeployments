SELECT 
    I.sensorId,
    Collect(I.[dateTime]) as [timestamps],
    Collect(I.opticalLFiltered) as [opticalLFiltered],
    Collect(I.opticalRFiltered) as [opticalRFiltered],
    Collect(I.pumpPressure) as [pumpPressure],
    Collect(I.rotational) as [rotational],
    Collect(I.vibrationHorizon) as [vibrationHorizon]
INTO AnomalyDetectionFunction
FROM IoTInput I TIMESTAMP BY I.[dateTime]
GROUP BY I.sensorId, HoppingWindow(minute, 60, 10)